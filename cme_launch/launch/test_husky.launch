<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<arg name="gazebo" default="true"/>
	<arg name="playpen" default="true"/>
	<!-- husky settings -->
	<arg name="laser_enabled" default="true"/>
	<arg name="realsense_enabled" default="true"/>

	<include if="$(arg playpen)" file="$(find husky_gazebo)/launch/playpen.launch"/>
	<include unless="$(arg playpen)" file="$(find gazebo_ros)/launch/empty_world.launch">
		<arg name="world_name" value="$(arg world_name)"/>
		<arg name="paused" value="false"/>
		<arg name="use_sim_time" value="true"/>
		<arg name="gui" value="true"/>
		<arg name="headless" value="false"/>
		<arg name="debug" value="false"/>
	</include>

	<include file="$(find husky_viz)/launch/view_robot.launch"/>

	<include file="$(find husky_navigation)/launch/amcl_demo.launch"/>
</launch>

